#include <iostream>
#include <conio.h>
#include <iomanip>
using namespace std;
int main()
{
	cout << "250*5750= " << 250*5750 << endl;
	cout << "100*28000= " << 100*28000 << endl;
	cout << "50*35750= " << 50*35750 << endl;
	cout << "25*18750= " << 25*18750 << endl;

	cout << "Ticket Sold= "  << setprecision(2) << 1437500+2800000+1787500+468750 << endl;
	
-getch();
return 0;
}
